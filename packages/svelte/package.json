{
  "name": "@haze-ui/svelte",
  "version": "0.0.2",
  "scripts": {
    "build": "svelte-package -i src && tsc ../types/*.ts --outDir dist/types --declaration --module esnext --skipLibCheck && rm ../types/*d.ts"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "svelte": "^5.0.0",
    "@sveltejs/package": "^2.0.0"
  },
  "keywords": [
    "svelte"
  ],
  "publishConfig": {
    "access": "public"
  }
}
